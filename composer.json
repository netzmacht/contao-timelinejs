{
    "name":"netzmacht/contao-timelinejs",
    "description":"Integrates ",
    "keywords":["TimelineJS", "Timeline", "contao"],
    "type":"contao-module",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"David Molineus",
            "email":"mail@netzmacht.de",
            "homepage":"http://www.netzmacht.de",
            "role":"Developer"
        }
    ],
    "support":{
        "email":"mail@netzmacht.de",
        "issues":"https://github.com/netzmacht/contao-timelinejs/issues",
        "source":"https://github.com/netzmacht/contao-timelinejs"
    },
    "require":{
        "php":">=5.4",
        "contao/core":"~3.2",
        "contao-community-alliance/meta-palettes":"~1.4",
        "netzmacht/contao-toolkit": "~1",
        "netzmacht/contao-javascript-builder": "~1",
        "beberlei/assert": "~2"
    },
    "require-dev": {
        "netzmacht/contao-build-tools": "~1.1"
    },
    "replace": {
        "contao-legacy/timelinejs": "*"
    },
    "autoload": {
        "psr-4": {
            "Netzmacht\\Contao\\TimelineJs\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra":{
        "contao": {
            "sources":{
                "module": "system/modules/timelinejs"
            }
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "3.0.x-dev"
        }
    }
}
